  #LinaroCloud
  # limits: 10 vms, 128 Cores, 132000 M Ram
  # For flavor limits, linaro can only create 8 vms at most.
  # But there is a internal test ARM instance in allinone, so there will be 7 ARM instances providered by linaro for online env.
  - name: linaro-openlab
    cloud: linaro
    driver: openstack
    launch-timeout: 1200
    boot-timeout: 1200
    max-concurrency: 10
    cloud-images:
      - name: ubuntu-xenial-arm64
        image-name: c394b90b-2b16-448c-91db-7f451eefda0c
    pools:
      - name: ubuntu-xenial-linaro
        max-ram: 81920
        networks:
          - openlab-net
        security-groups:
          - openlab-sg
        labels:
          - name: ubuntu-xenial-arm64
            cloud-image: ubuntu-xenial-arm64
